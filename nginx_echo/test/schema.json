{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "request": {
            "type": "object",
            "properties": {
                "uri": {
                    "type": "object",
                    "properties": {
                        "headers": {
                            "type": "object",
                            "additionalProperties": true,
                            "optional": true
                        },
                        "method": {
                            "type": "string",
                            "enum": ["GET", "HEAD", "POST", "PUT", "DELETE", "MKCOL", "COPY", "MOVE", "OPTIONS", "PROPFIND", "PROPPATCH", "LOCK", "UNLOCK", "PATCH"]
                        },
                        "scheme": {
                            "type": "string"
                        },
                        "path": {
                            "type": "string"
                        },
                        "fullPath": {
                            "type": "string"
                        },
                        "queryString": {
                            "type": "object",
                            "additionalProperties": true,
                            "optional": true
                        },
                        "body": {
                            "type": "string",
                            "optional": true
                        }
                    },
                    "additionalProperties": false,
                    "required": [
                        "method",
                        "scheme",
                        "path",
                        "fullPath"
                    ]
                },
                "network": {
                    "type": "object",
                    "properties": {
                        "clientPort": {
                            "type": "string"
                        },
                        "clientAddress": {
                            "type": "string"
                        },
                        "serverAddress": {
                            "type": "string"
                        },
                        "serverPort": {
                            "type": "string"
                        }
                    },
                    "additionalProperties": false,
                    "required": [
                        "clientPort",
                        "clientAddress",
                        "serverAddress",
                        "serverPort"
                    ]
                },
                "ssl": {
                    "type": "object",
                    "properties": {
                        "isHttps": {
                            "type": "boolean"
                        },
                        "sslProtocol":{
                            "type": "string",
                            "optional": true,
                            "enum": ["SSLv2","SSLv3","TLSv1","TLSv1.1","TLSv1.2","TLSv1.3"]
                        },
                        "sslCipher": {
                            "type": "string",
                            "optional": true
                        }
                    },
                    "additionalProperties": false,
                    "required": [
                        "isHttps"
                    ]
                },
                "session": {
                    "type": "object",
                    "properties": {
                        "requestId": {
                            "type": "string"
                        },
                        "connection": {
                            "type": "string"
                        },
                        "connectionNumber": {
                            "type": "string"
                        }
                    },
                    "additionalProperties": false,
                    "required": [
                        "requestId",
                        "connection",
                        "connectionNumber"
                    ]
                }
            },
            "additionalProperties": false,
            "required": [
                "uri",
                "network",
                "ssl",
                "session"
            ]
        },
        "environment":{
            "type": "object",
            "properties": {
                "machineName": {
                    "type": "string"
                }
            },
            "additionalProperties": false,
            "required": [
                "machineName"
            ]
        },
        "response": {
            "type": "object",
            "properties": {
                "addedDelay": {
                    "type": "object",
                    "properties": {
                        "status": {
                            "type": "string",
                            "enum": ["ENABLED","DISABLED"]
                        },
                        "min":{
                            "type": "integer"
                        },
                        "max": {
                            "type": "integer"
                        },
                        "actual":{
                            "type": "integer"
                        },
                        "timeStamp": {
                            "type": "string"
                        }
                    },
                    "additionalProperties": false,
                    "required": [
                        "status"
                    ]
                },
                "body": {
                    "type": "string"
                },
                "statusCode": {
                    "type": "integer"
                },
                "statusReason": {
                    "type": "string"
                },
                "statusBody": {
                    "type": "string"
                },
                "timeStamp": {
                    "type": "string"
                }
            },
            "additionalProperties": false,
            "required": [
                "body",
                "addedDelay",
                "statusCode",
                "statusReason",
                "statusBody",
                "timeStamp"
            ]
        }
    },
    "additionalProperties": false,
    "required": [
        "request",
        "environment",
        "response"
    ]
}